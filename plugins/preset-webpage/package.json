{
  "name": "ezygrapes-preset-webpage",
  "version": "0.0.24",
  "description": "Ezygrapes default editor plugin",
  "main": "dist/ezygrapes-preset-webpage.min.js",
  "scripts": {
    "lint": "eslint src",
    "v:patch": "npm version --no-git-tag-version patch",
    "build": "npm run v:patch && webpack --config webpack.prod.js && npm run build:css",
    "start": "webpack serve --open --config webpack.dev.js & npm run build:css -- -w",
    "build:css": "node-sass src/scss/main.scss dist/ezygrapes-preset-webpage.min.css --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/youngmonkeys/ezygrapes.git"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "webpage",
    "preset",
    "wysiwyg"
  ],
  "author": "Artur Arseniev",
  "license": "BSD-3-Clause",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "eslint": "^8.35.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^8.0.0",
    "webpack": "^5.76.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "regenerator-runtime": "^0.13.11"
  }
}
